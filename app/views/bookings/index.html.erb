<div class="container mt-5">

  <div class="row">
    <div class="col-sm-6">
      <h1>Bookings for my Boats</h1>
      <ul>
        <% @incomingbooking.each do |booking| %>
        <li><%= booking %></li>
        <% end %>
      </ul>
    </div>

    <div class="col-sm-6">
      <h1>My Bookings for other Boats </h1>
      <ul>
        <% @externalbookingrequest.each do |booking| %>
        <li><%= booking %></li>
        <% end %>
      </ul>
    </div>


  </div>

</div>
